push 0          call test_print
push -0         call test_print
push -6         call test_print
push 7          call test_print
push 255        call test_print
push 3735928559 call test_print

end

# (x -- )
test_print:
    call ^ 2 printi_base printc ' '
    call ^ 2 printi_base_prefixed printc ' '
    call ^ 8 printi_base printc ' '
    call ^ 8 printi_base_prefixed printc ' '
    call ^ 16 printi_base printc ' '
    call ^ 16 printi_base_prefixed printc '\n'
    drop
    ret
