    "io/format_int" import

    0          call test_printi
    -0         call test_printi
    -6         call test_printi
    7          call test_printi
    255        call test_printi
    3735928559 call test_printi
    end

# (x -- )
test_printi:
    ^ 2  call io.printi_base ' ' printc
    ^ 2  call io.printi_base_prefixed ' ' printc
    ^ 8  call io.printi_base ' ' printc
    ^ 8  call io.printi_base_prefixed ' ' printc
    ^ 16 call io.printi_base ' ' printc
    ^ 16 call io.printi_base_prefixed '\n' printc
    drop ret
